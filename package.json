{
  "name": "ng-feedhenry",
  "version": "0.2.53",
  "main": "./dist/ngFH.js",
  "description": "AngularJS friendly FeedHenry SDK wrapper.",
  "repository": "feedhenry-staff/ngFeedHenry",
  "scripts": {
    "clean": "rm -r dist test/bundle.js",
    "build": "npm run lint && ./node_modules/.bin/browserify -e ./src/ngFH.js -o ./dist/ngFH.js",
    "debugbuild": "./node_modules/.bin/browserify -e ./src/ngFH.js -o ./test/bundle.js",
    "prepublish": "npm run build",
    "lint": "./node_modules/.bin/eslint .",
    "test": "npm run debugbuild && ./node_modules/karma/bin/karma start"
  },
  "authors": [
    "Evan Shortiss"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular": "~1.5.8",
    "angular-mocks": "~1.5.8",
    "browserify": "~4.1.8",
    "eslint": "~3.9.1",
    "fh-mocks": "0.1.1",
    "karma": "~1.2.0",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-mocha": "~1.0.1",
    "karma-mocha-reporter": "~2.0.4"
  },
  "dependencies": {
    "xtend": "^4.0.0",
    "fhlog": "~0.1.6",
    "route-matcher": "0.1.0",
    "brfs": "~1.2.0"
  }
}
